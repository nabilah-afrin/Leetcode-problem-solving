   "source": [
    "class Solution:\n",
    "    def haystack_needle(self, s,t):\n",
    "        for i in range(len(s)-len(t)+1):\n",
    "            if s[i:i+len(t)] == t:\n",
    "                return i\n",
    "        return -1\n",
    "    "
   ]
  },
